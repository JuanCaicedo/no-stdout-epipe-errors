{
  "name": "no-stdout-epipe-errors",
  "description": "Remove epipe errors when piping commands together",
  "version": "0.0.0",
  "author": "Juan Caicedo @JuanCaicedo",
  "bugs": "https://github.com/JuanCaicedo/no-stdout-epipe-errors/issues",
  "dependencies": {
    "@oclif/command": "^1.4.6",
    "@oclif/config": "^1.3.62"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.4.2",
    "@oclif/plugin-help": "^1.2.2",
    "@types/chai": "^4.1.2",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.1",
    "globby": "^8.0.1",
    "ts-node": "^5.0.1",
    "tslib": "^1.9.0",
    "typescript": "^2.8.1"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    ".oclif.manifest.json",
    "/lib"
  ],
  "homepage": "https://github.com/JuanCaicedo/no-stdout-epipe-errors",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "oclif-example",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "JuanCaicedo/no-stdout-epipe-errors",
  "scripts": {
    "build": "rm -rf lib && tsc",
    "clean": "rm -f .oclif.manifest.json",
    "lint": "tsc -p . --noEmit",
    "postpublish": "npm run clean",
    "posttest": "npm run lint",
    "prepublishOnly": "npm run build && oclif-dev manifest",
    "preversion": "npm run clean",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  }
}
